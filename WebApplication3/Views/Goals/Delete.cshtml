@model FinanceAnalytic.Models.Models.Goal

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    

    <h3>Вы уверены что хотите удалить?</h3>
    <div>
        <h4>Цель</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Name)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Sum)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Sum)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.User)
            </dt>

        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Удалить" class="button_yellow_s"/> |
            <a asp-action="Index" class="button_yellow_s">Назад к списку</a>
        </form>
    </div>
</div>