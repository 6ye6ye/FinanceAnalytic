@model FinanceAnalytic.Models.Income

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    

    <h3>Подтвердите удаление</h3>
    <div>
        <h4>Доход</h4>
        <hr />
        <dl class="row">
       
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Date)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Date)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IncomeCategory)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.IncomeCategory.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IncomeSubCategory)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.IncomeSubCategory.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Sum)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Sum)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Currency)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Currency.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CurrencyRate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CurrencyRate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SumInCurrency)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SumInCurrency)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Comments)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Comments)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Удалить" class="button_yellow_s"/> |
            <a asp-action="Index" class="button_yellow_s">Вернуться к списку</a>
        </form>
    </div>
</div>